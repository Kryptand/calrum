{
  "name": "calrum",
  "version": "0.9.0",
  "contributors": [
    "Kryptand"
  ],
  "license": "MIT",
  "repository": "Kryptand/calrum",
  "scripts": {
    "build:analyze": "webpack --env production --analyze",
    "build": "webpack --env production",
    "client": "webpack-dev-server --env development",
    "server": "nodemon server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "lint:js": "eslint src/app/*.js src/views/*.js src/styles/*.js",
    "lint:css": "stylelint --syntax html src",
    "lint": "npm-run-all --parallel lint:*",
    "start": "node server.js",
    "build:webpack": "rm -rf dist && webpack --config webpack.config.js",
    "serve": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "serve:webpack": "polymer serve --port 3000 dist",
    "test": "npm run test:unit && npm run test:integration",
    "postinstall": "node utils/generate-helpers.js && npm run build",
    "test:integration": "tsc && mocha test/integration --timeout=10000",
    "test:unit": "tsc && polymer test --module-resolution=node --npm",
    "test:sauce": "tsc && polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "tsc && mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.0",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/polymer": "^3.1.0",
    "@types/redux-persist": "^4.3.1",
    "@vaadin/router": "^1.3.0",
    "@vaadin/vaadin": "^14.0.1",
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "lit-element": "^2.0.1",
    "localforage": "^1.7.3",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@polymer/test-fixture": "^4.0.2",
    "connect-history-api-fallback": "^1.6.0",
    "express": "^4.17.1",
    "express-static-gzip": "^2.0.3",
    "axe-core": "^3.0.0",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "mocha": "^5.2.0",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.9.3",
    "polyserve": "^0.27.0",
    "prpl-server": "^1.4.0",
    "puppeteer": "^1.5.0",
    "tslint": "^5.18.0",
    "typescript": "^3.1.3",
    "wct-mocha": "^1.0.0",
    "@babel/plugin-external-helpers": "^7.2.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-template-html-minifier": "^3.1.0",
    "babel-preset-minify": "^0.5.1",
    "brotli-webpack-plugin": "^1.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.2",
    "copy-webpack-plugin": "^5.0.4",
    "html-webpack-plugin": "^3.2.0",
    "lint-staged": "^9.2.3",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "terser-webpack-plugin": "^1.4.1",
    "uglify-template-string-loader": "^1.1.1",
    "webpack": "^4.39.2",
    "webpack-babel-multi-target-plugin": "^2.3.0",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.1",
    "workbox-webpack-plugin": "^4.3.1"
  }
}
