{
  "name": "calrum",
  "version": "0.9.0",
  "contributors": [
    "Kryptand"
  ],
  "license": "MIT",
  "repository": "Kryptand/calrum",
  "scripts": {
    "start": "gulp serve",
    "start:webpack": "webpack-dev-server --mode development",
    "build": "tsc && polymer build --auto-base-path && gulp prpl-server",
    "build:static": "tsc && polymer build",
    "build:webpack": "del dist&& webpack --config webpack.config.js",
    "serve": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "serve:webpack": "polymer serve --port 3000 dist",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "tsc && mocha test/integration --timeout=10000",
    "test:unit": "tsc && polymer test --module-resolution=node --npm",
    "test:sauce": "tsc && polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "tsc && mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.0",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/polymer": "^3.1.0",
    "@types/redux-persist": "^4.3.1",
    "@vaadin/router": "^1.3.0",
    "@vaadin/vaadin": "^14.0.1",
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "lit-element": "^2.0.1",
    "localforage": "^1.7.3",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "ts-loader": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@polymer/test-fixture": "^4.0.2",
    "axe-core": "^3.3.2",
    "babel-loader": "^8.0.4",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^5.0.4",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.9.11",
    "polyserve": "^0.27.5",
    "prpl-server": "^1.4.0",
    "puppeteer": "^1.19.0",
    "typescript": "^3.5.3",
    "wct-mocha": "^1.0.0",
    "webpack": "4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "workbox-webpack-plugin": "^4.3.1"
  }
}
